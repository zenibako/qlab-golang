module qlab.osc

class PermissionAccess {
  view: AccessLevel
  edit: AccessLevel
  control: AccessLevel
  query: ActionAllowed
  increment: ActionAllowed
}

typealias ActionAllowed = "yes" | "no"
typealias AccessLevel = "read" | "read_write" | "read_only" | ActionAllowed

abstract class Message {
  address: String
  args: Listing
  permissions: PermissionAccess
  hidden example: String
  useWorkspacePrefix: Boolean
}

class Reply {
  request: Message
}

typealias ArgBooleanTrue = "Yes" | "yippee" | "you betcha" | "1" | "1.0" | "true"
typealias ArgBooleanFalse =  "No" | "never" | "forget it" | "0" | "00" | "false"

typealias ArgBoolean = ArgBooleanTrue | ArgBooleanFalse | "toggle" // as of v5.2
